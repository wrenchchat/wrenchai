{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github",
        "env",
        "${keychain:mcp-servers:github-mcp:pat}"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "seq-think": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },    
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@Deploya-labs/mcp-supabase",
        "--config",
        "{\"supabaseProjectRef\":\"${keychain:mcp-servers:supabase:project-ref}\",\"supabaseDbPassword\":\"${keychain:mcp-servers:supabase:db-password}\"}"
      ]
    },
    "browser-tools": {
      "command": "npx",
      "args": [
        "@agentdeskai/browser-tools-mcp@1.1.0"
      ]
    },
    "taskmaster-ai": {
      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"],
      "env": {
        "anthropic_api_key": "${keychain:anthropic_api_key:lim}",
        "gemini_api_key": "${keychain:gemini_api_key:lim}",
        "MODEL": "claude-3-7-sonnet-20250219",
        "PERPLEXITY_MODEL": "sonar-pro",
        "MAX_TOKENS": "64000",
        "TEMPERATURE": "0.2",
        "DEFAULT_SUBTASKS": "5",
        "DEFAULT_PRIORITY": "medium"
      }
    }
  },
  "unconfiguredServers": {
    "mac-apps": "@modelcontextprotocol/server-mac-apps",
    "docker": "@modelcontextprotocol/server-docker",
    "brave": "modelcontextprotocol/server-brave-search",
    "playwright": "@modelcontextprotocol/server-playwright",
    "stripe": "@stripe/mcp",
    "chroma": "@modelcontextprotocol/inspector",
    "google-calendar": "@modelcontextprotocol/server-google-calendar",
    "gmail": "@gongrzhe/server-gmail-autoauth-mcp"
  }
}